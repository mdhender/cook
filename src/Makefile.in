cooktime/date.gen.c cooktime/date.gen.h: cooktime/date.y
	@echo Expect 8 shift/reduce conflicts:
	$(YACC) -d cooktime/date.y
	sed -e 's/[yY][yY]/date_/g' -e '/<stdio.h>/d' -e '/<stdlib.h>/d' \
		-e '/<stddef.h>/d' y.tab.c > cooktime/date.gen.c
	sed -e 's/[yY][yY]/date_/g' -e 's/Y_TAB_H/date_TAB_H/g' y.tab.h \
		> cooktime/date.gen.h
	rm y.tab.c y.tab.h

