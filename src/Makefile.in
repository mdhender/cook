make2cook/gram.gen.c make2cook/gram.gen.h: make2cook/gram.y
	@echo Expect 2 shift/reduce conflicts:
	$(YACC) -d make2cook/gram.y
	sed -e 's/[yY][yY]/gram_/g' -e '/<stdio.h>/d' -e '/<stdlib.h>/d' \
		-e '/<stddef.h>/d' y.tab.c > make2cook/gram.gen.c
	sed -e 's/[yY][yY]/gram_/g' -e 's/Y_TAB_H/gram_TAB_H/g' y.tab.h \
		> make2cook/gram.gen.h
	rm y.tab.c y.tab.h

