make2cook/vargram.gen.c make2cook/vargram.gen.h: make2cook/vargram.y
	@echo Expect 2 shift/reduce, 10 reduce/reduce conflicts:
	$(YACC) -d make2cook/vargram.y
	sed -e 's/[yY][yY]/vargram_/g' -e '/<stdio.h>/d' -e \
		'/<stdlib.h>/d' -e '/<stddef.h>/d' y.tab.c > \
		make2cook/vargram.gen.c
	sed -e 's/[yY][yY]/vargram_/g' -e 's/Y_TAB_H/vargram_TAB_H/g' \
		y.tab.h > make2cook/vargram.gen.h
	rm y.tab.c y.tab.h

